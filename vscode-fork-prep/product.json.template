        }
      }
    },
    {
      "name": "aura-ai",
      "version": "1.0.0", 
      "repo": "https://github.com/aura-ai/aura-ai",
      "metadata": {
        "id": "aura.ai",
        "displayName": "Aura AI Assistant",
        "description": "Advanced AI coding assistant with multiple model support",
        "publisherId": {
          "publisherId": "aura-ai",
          "publisherName": "Aura AI",
          "displayName": "Aura AI",
          "flags": "verified"
        }
      }
