  "webEndpointUrl": "https://aura-ai.dev",
  "webEndpointUrlTemplate": "https://aura-ai.dev/{version}/aura",
  "linkProtectionTrustedDomains": [
    "https://aura-ai.dev",
    "https://github.com/aura-ai",
    "https://docs.aura-ai.dev"
  ],
  "commandPaletteSuggestedCommandIds": [
    "aura.ai.chat",
    "aura.ai.generateCode", 
    "aura.ai.explainCode",
    "aura.ai.refactorCode",
    "aura.3d.toggleView",
    "aura.3d.immersiveMode",
    "aura.swarm.createTask",
    "aura.swarm.orchestrate",
    "workbench.action.quickOpen",
    "workbench.action.files.newFile"
  ],
  "tunnelApplicationName": "aura",
  "tunnelApplicationConfig": {
    "authenticationProviders": {
      "github": {
        "scopes": [
          "user:email",
          "read:user"
        ]
      }
    },
    "editSessionId": "aura",
    "supportsUniversalLinks": true
  },
  "aiConfig": {
    "enableInlineCompletions": true,
    "enableChatAssistant": true,
    "enable3DVisualization": true,
    "enableSwarmOrchestration": true,
    "defaultModel": "gpt-4",
    "supportedProviders": [
      "openai",
      "anthropic", 
      "local"
    ]
  },
  "serverLicense": [
    "commercial"
  ],
  "serverLicenseUrl": "https://aura-ai.dev/license",
  "quality": "stable",
  "commit": "{{COMMIT_HASH}}",
  "date": "{{BUILD_DATE}}",
  "version": "{{VERSION}}"
