        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-commonjs": "^25.0.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "concurrently": "^8.2.0",
        "rimraf": "^5.0.5"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "publishConfig": {
        "access": "restricted",
        "registry": "https://npm.g3d.ai"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/g3d-ai/shared-infrastructure.git"
