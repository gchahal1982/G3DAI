{
    "name": "@g3d/shared-infrastructure",
    "version": "1.0.0",
    "description": "Shared infrastructure components for all G3D AI services",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc && npm run build:ui",
        "build:ui": "rollup -c",
        "dev": "concurrently \"tsc -w\" \"rollup -c -w\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "clean": "rimraf dist"
    },
    "keywords": [
        "g3d",
        "ai",
        "infrastructure",
        "authentication",
        "ui-components",
        "shared"
    ],
    "author": "G3D AI Team",
    "license": "MIT",
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^6.1.0",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "crypto-js": "^4.2.0",
        "uuid": "^9.0.1",
        "axios": "^1.6.0",
        "socket.io-client": "^4.7.4",
        "date-fns": "^2.30.0",
        "lodash": "^4.17.21",
        "zod": "^3.22.4",
        "react-hook-form": "^7.48.0",
        "react-query": "^3.39.3",
        "framer-motion": "^10.16.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/styled-components": "^5.1.0",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/bcryptjs": "^2.4.6",
        "@types/crypto-js": "^4.2.1",
        "@types/uuid": "^9.0.7",
        "@types/lodash": "^4.14.0",
        "@types/jest": "^29.5.0",
        "@types/node": "^20.10.0",
        "typescript": "^5.0.2",
        "eslint": "^8.55.0",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/jest-dom": "^6.1.0",
        "@testing-library/user-event": "^14.5.0",
        "rollup": "^4.6.0",
