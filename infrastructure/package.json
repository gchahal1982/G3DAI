  "description": "G3D AI Services - Production Backend Infrastructure",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t g3d-backend .",
    "docker:run": "docker run -p 3000:3000 g3d-backend",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run"
  },
  "keywords": [
    "ai",
    "microservices",
    "backend",
    "api",
    "g3d",
    "enterprise",
    "saas"
